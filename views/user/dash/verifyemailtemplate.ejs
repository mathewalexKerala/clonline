


    <!-- Modal -->
    <div class="modal fade bg-white" id="templatemo_search" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="w-100 pt-1 mb-5 text-right">
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <form action="" method="get" class="modal-content modal-body border-0 p-0">
                <div class="input-group mb-2">
                    <input type="text" class="form-control" id="inputModalSearch" name="q" placeholder="Search ...">
                    <button type="submit" class="input-group-text bg-success text-light">
                        <i class="fa fa-fw fa-search text-white"></i>
                    </button>
                </div>
            </form>
        </div>
    </div>


<div style="position: fixed; top:0; left:0; right:0; z-index: 9;" id="dashResponse"></div>
    <!-- Open Content -->
    <section class="bg-light bgimage1" > 
        <div class="container py-5">
            <div class="container-fluid px-0">
                <h2 class="text-muted text-capitalize text-center">
                    <%= dashPage %>
                </h2>
            </div>
            <div class="row">
                <!-- col end -->
                <div class="col-lg-8 mx-auto "> 
                    <div class="card">
                        <div class="card-body">
                            <div class="border p-2">
                                <% if(emailVerificationSuccess){ %> 
                                    <div class="d-flex">
                                        <h5 class="text-secondary  mb-2 text-center  d-inline">Email verification! </h5> 
                                        <i class="bi bi-envelope-check d-inline ms-auto h4 text-secondary text-success"></i>
                                    </div>
                                    <div class="border text-success text-center p-3">
                                        <p>You have verified your email with us successfully! We are going to redirect you to the dashboard.</p>
                                        <a href="/dash/profile/" class="btn btn-primary btn-sm">Visit dashboard</a>
                                    </div>
                                    <% }else{ %> 
                                    <div class="d-flex">
                                        <h5 class="text-secondary  mb-2 text-center  d-inline">Email verification! </h5> 
                                        <i class="bi bi-envelope-exclamation-fill d-inline ms-auto h4 text-secondary text-danger"></i> 
                                        
                                    </div>
                                    <div class="border text-danger text-center p-3">
                                        <% if(userData.email_verified){ %> 
                                            <p>You are already validated your email with us! you don't want to try again!</p>
                                            <a href="/" class="btn btn-primary btn-sm">Go home</a>
                                        <% }else{ %> 
                                        <p>We couldn't verify your email, please try again! We are going to redirect you to the verification page.</p>
                                        <a href="/dash/verify/" class="btn btn-primary btn-sm">Resend Email</a>
                                        <% } %>
                                    </div>
                                    <% } %>
                                    
                               
                               
                               
                            </div>
                            

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Close Content -->

